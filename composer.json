{
    "name": "atyagi/elasticache-laravel",
    "description": "Elasticache Drivers for Laravel Cache and Session",
    "keywords": ["laravel", "AWS", "Elasticache"],
    "license": "MIT",
    "authors": [
        {
            "name": "Ankit Tyagi",
            "email": "ankit.tyagi.2@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "5.0.*",
        "illuminate/contracts": "5.0.*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.6.*",
        "mockery/mockery": "dev-master",
        "satooshi/php-coveralls": "dev-master"
    },
    "suggest": {
        "ext-memcached": "Leverage the Memcached extension for Elasticache connections."
    },
    "autoload": {
        "psr-0": {
            "Atyagi\\Elasticache": "src/"
        }
    },
    "minimum-stability": "dev"
}
